{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended",
      "plugin:jest/recommended"
    ],
    "plugins": ["prettier", "jest"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  }